								},
								"sink": {
									"name": "Product",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "SalesValue",
									"type": "String"
								},
								"sink": {
									"name": "SalesValueUSD",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"name": "UnitsSold",
